{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["dom", "DOM.Iterable", "es2017"],
    "module": "ES6",
    "rootDir": "./src",
    "outDir": "./dist",
    "baseUrl": "./src",
    "paths": {
      "modules/*": ["modules/*"]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true
  }
}
